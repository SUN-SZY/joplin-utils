{
  "name": "joplin-api",
  "version": "0.6.1",
  "description": "joplin ts/js data api",
  "license": "mit",
  "homepage": "https://joplin-utils.rxliuli.com/api/joplin-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/joplin-utils/tree/master/libs/joplin-api"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "docs-build": "rimraf docs && typedoc src/index.ts",
    "docs-deploy": "pnpm docs-build && gh-pages -d docs -e /api/joplin-api --add",
    "docs-dev": "pnpm docs-build && live-server docs",
    "prepublishOnly": "pnpm build",
    "setup": "pnpm build",
    "test": "vitest run"
  },
  "dependencies": {
    "@liuli-util/object": "^3.5.1",
    "lodash-es": "^4.17.21",
    "query-string": "^9.0.0"
  },
  "devDependencies": {
    "@liuli-util/eslint-config-ts": "^0.4.0",
    "@liuli-util/vite-plugin-node": "^0.9.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.13.0",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.1.1",
    "joplin-plugin-api": "workspace:^",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.5.4",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "deploy": {
    "type": "gh-pages",
    "dist": "docs",
    "dest": "/api/joplin-api"
  }
}
