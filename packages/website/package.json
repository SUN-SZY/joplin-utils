    "@types/node": "^20.13.0",
    "gh-pages": "^6.1.1",
    "lodash-es": "^4.17.21",
    "typedoc": "^0.27.6",
    "typedoc-plugin-markdown": "^4.4.1",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "^5.5.4",
    "vite-node": "^2.0.5",
    "vitepress": "1.2.2",
    "vitepress-plugin-twitter-card": "^0.1.0",
    "vitepress-sidebar": "^1.23.2",
    "vue": "^3.4.27",
    "zx": "^8.1.4"
