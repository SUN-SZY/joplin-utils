{
  "name": "joplin-plugin-joplin-publisher",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "pnpm build --watch",
    "dist": "webpack --env joplin-plugin-config=buildMain && webpack --env joplin-plugin-config=buildExtraScripts && webpack --env joplin-plugin-config=createArchive",
    "setup": "pnpm dist",
    "updateVersion": "webpack --env joplin-plugin-config=updateVersion",
    "update": "npm install -g generator-joplin && yo joplin --node-package-manager npm --update --force"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin"
  ],
  "files": [
    "publish"
  ],
  "devDependencies": {
    "@joplin/lib": "~2.9",
    "@liuli-util/async": "^3.7.0",
    "@liuli-util/markdown-util": "^0.9.2",
    "@liuli-util/vite-plugin-node": "^0.9.0",
    "@mark-magic/core": "^0.12.0",
    "@mark-magic/plugin-hexo": "^0.14.0",
    "@mark-magic/plugin-joplin": "^0.12.7",
    "@mark-magic/utils": "^0.12.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^18.7.13",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "env-paths": "^3.0.0",
    "filenamify": "^6.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "globby": "^14.0.1",
    "isomorphic-git": "^1.25.10",
    "joplin-api": "^0.5.1",
    "mime-types": "^2.1.35",
    "pathe": "^1.1.2",
    "tar": "^6.1.11",
    "temp-dir": "^3.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^5.4.5",
    "vite": "^5.1.3",
    "vite-plugin-static-copy": "^1.0.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
