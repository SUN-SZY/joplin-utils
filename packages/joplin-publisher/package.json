    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.13.0",
    "env-paths": "^3.0.0",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.1",
    "isomorphic-git": "^1.25.10",
    "joplin-api": "workspace:^",
    "joplin-plugin-api": "workspace:^",
    "lodash-es": "^4.17.21",
    "tar": "^7.2.0",
    "typescript": "^5.4.5",
