{
  "name": "joplin-blog",
  "version": "0.9.0",
  "license": "MIT",
  "type": "module",
  "bin": "./bin.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts && tsup src/bin.ts --format esm --target esnext",
    "dev": "tsup-node src/index.ts --format esm,cjs --dts && tsup src/bin.ts --format esm --target esnext --watch",
    "gen-i18n": "i18next-dts-gen --dirs src/i18n",
    "setup": "pnpm run gen-i18n && pnpm run build",
    "start": "tsx src/bin.ts"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@liuli-util/array": "^3.5.0",
    "@liuli-util/async": "^3.6.0",
    "@liuli-util/fs-extra": "^0.1.0",
    "@liuli-util/i18next-util": "^0.4.1",
    "@liuli-util/object": "^3.5.0",
    "@liuli-util/tree": "^3.5.0",
    "comlink": "^4.3.1",
    "commander": "^9.4.1",
    "dayjs": "^1.11.5",
    "figlet": "^1.5.2",
    "form-data": "^4.0.0",
    "i18next": "^21.9.2",
    "joplin-api": "workspace:*",
    "mdast": "^3.0.0",
    "node-fetch": "^3.2.10",
    "ora": "^6.1.2",
    "os-locale": "^6.0.2",
    "prettier": "^2.7.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^10.0.2",
    "unified": "^10.1.2",
    "unist-util-map": "^3.1.2",
    "unist-util-visit": "^4.1.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@liuli-util/i18next-dts-gen": "^0.6.3",
    "@rollup/plugin-json": "^4.1.0",
    "@types/figlet": "^1.5.5",
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.7.23",
    "@types/prettier": "^2.7.1",
    "@types/shelljs": "^0.8.11",
    "@types/unist": "^2.0.6",
    "builtin-modules": "^3.3.0",
    "envfile": "^6.18.0",
    "moment": "^2.29.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-add-shebang": "^0.3.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-node-externals": "^5.0.0",
    "rollup-plugin-typescript2": "^0.34.0",
    "rollup-plugin-worker-threads": "^0.3.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "tsx": "^3.9.0",
    "type-fest": "^3.0.0",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
