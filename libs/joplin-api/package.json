{
  "name": "joplin-api",
  "version": "0.4.2",
  "description": "joplin ts/js data api",
  "license": "mit",
  "homepage": "https://joplin-utils.rxliuli.com/api/joplin-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/joplin-utils/tree/master/libs/joplin-api"
  },
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "docs-build": "rimraf docs && typedoc",
    "docs-deploy": "pnpm docs-build && liuli-cli deploy",
    "docs:dev": "pnpm docs-build && live-server docs",
    "prepublishOnly": "pnpm build",
    "setup": "pnpm build",
    "test": "vitest run -w=1"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@liuli-util/object": "^3.2.7",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.1",
    "query-string": "^6.13.7"
  },
  "devDependencies": {
    "@liuli-util/eslint-config-ts": "^0.2.0",
    "@types/fs-extra": "^9.0.4",
    "@types/node-fetch": "^2.5.7",
    "envfile": "^6.17.0",
    "fs-extra": "^9.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "tsup": "^6.2.3",
    "typedoc": "^0.21.2",
    "typescript": "^4.6.3",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "deploy": {
    "type": "gh-pages",
    "dist": "docs",
    "dest": "/api/joplin-api"
  },
  "eslintConfig": {
    "extends": [
      "@liuli-util/eslint-config-ts"
    ]
  }
}
