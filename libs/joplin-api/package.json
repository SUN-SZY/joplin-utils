{
  "name": "joplin-api",
  "version": "0.3.3",
  "description": "joplin ts/js data api",
  "license": "mit",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "pnpm build",
    "setup": "pnpm build",
    "build": "rimraf dist && liuli-cli build pkg",
    "dev": "liuli-cli build pkg -w",
    "docs:dev": "pnpm docs-build && live-server docs",
    "docs-build": "rimraf docs && typedoc",
    "docs-deploy": "pnpm docs-build && liuli-cli deploy"
  },
  "deploy": {
    "type": "gh-pages",
    "dist": "docs",
    "dest": "/api/joplin-api"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/jest-start.ts"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://joplin-utils.rxliuli.com/api/joplin-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/rxliuli/joplin-utils/tree/master/libs/joplin-api"
  },
  "eslintConfig": {
    "extends": [
      "@liuli-util/eslint-config-ts"
    ]
  },
  "devDependencies": {
    "@liuli-util/cli": "^3.19.2",
    "@liuli-util/eslint-config-ts": "^0.2.0",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^27.0.2",
    "@types/node-fetch": "^2.5.7",
    "envfile": "^6.17.0",
    "fs-extra": "^9.0.1",
    "jest": "^27.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "typedoc": "^0.21.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@liuli-util/object": "^3.2.7",
    "axios": "^0.21.0",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.1",
    "query-string": "^6.13.7"
  }
}
